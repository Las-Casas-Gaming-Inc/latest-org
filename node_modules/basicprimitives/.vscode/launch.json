    "version": "0.2.0",
    "configurations": [
    {
      "command": "yarn test VisualTreeBuilder.test",
      "name": "Run npm start",
      "request": "launch",
      "type": "node-terminal"
    },
        
        {
            "name": "Debug Jest Tests",
            "type": "node",
            "request": "launch",
            "runtimeArgs": [
              "--inspect-brk",
              "${workspaceRoot}/node_modules/jest/bin/jest.js",
              "--runInBand"
            ],
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "port": 9229
          },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome against localhost",
            "url": "http://localhost:8080/samples/javascript.controls/DemoOrgDiagramMatrixLayout.html",
            "webRoot": "${workspaceFolder}"
        }
    ]
}